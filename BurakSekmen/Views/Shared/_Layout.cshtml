<!DOCTYPE html>
<html lang="zxx">

<head>
    <title>Burak Sekmen Rent A Car Yazılımı</title>
    <link rel="icon" href="images/icon.png" type="image/gif" sizes="16x16">
    <meta content="text/html;charset=utf-8" http-equiv="Content-Type">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="Rentaly - Multipurpose Vehicle Car Rental Website Template" name="description">
    <meta content="" name="keywords">
    <meta content="" name="author">
    <!-- CSS Files
    ================================================== -->
    <link href="~/site_tema/css/bootstrap.min.css" rel="stylesheet" type="text/css" id="bootstrap">
    <link href="~/site_tema/css/mdb.min.css" rel="stylesheet" type="text/css" id="mdb">
    <link href="~/site_tema/css/plugins.css" rel="stylesheet" type="text/css">
    <link href="~/site_tema/css/style.css" rel="stylesheet" type="text/css">
    <link href="~/site_tema/css/coloring.css" rel="stylesheet" type="text/css">
    <!-- color scheme -->
    <link id="colors" href="~/site_tema/css/colors/scheme-01.css" rel="stylesheet" type="text/css">
</head>

<body>
    <div id="wrapper">

        <!-- page preloader begin -->
        <div id="de-preloader"></div>
        <!-- page preloader close -->
        <!-- header begin -->
        <header class="transparent header-light scroll-light has-topbar clone smaller nav-upr" style="background-color:#fff">
            <div id="topbar" class="topbar-dark text-light">
                <div class="container">
                    <div class="topbar-left xs-hide">
                        <div class="topbar-widget">
                            <div class="topbar-widget"><a href="#"><i class="fa fa-phone"></i>+555 555 55 55</a></div>
                            <div class="topbar-widget"><a href="#"><i class="fa fa-envelope"></i>contact@rentacar.com</a></div>
                            <div class="topbar-widget"><a href="#"><i class="fa fa-clock-o"></i>Pazartesi - Pazar 08.00 - 18.00</a></div>
                        </div>
                    </div>

                    <div class="topbar-right">
                        <div class="social-icons">
                            <a href="#"><i class="fa fa-facebook fa-lg"></i></a>
                            <a href="#"><i class="fa fa-twitter fa-lg"></i></a>
                            <a href="#"><i class="fa fa-youtube fa-lg"></i></a>
                            <a href="#"><i class="fa fa-pinterest fa-lg"></i></a>
                            <a href="#"><i class="fa fa-instagram fa-lg"></i></a>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="de-flex sm-pt10">
                            <div class="de-flex-col">
                                <div class="de-flex-col">
                                    <!-- logo begin -->
                                    <div id="logo">
                                        <a asp-action="Index" asp-controller="Home">
                                            <img class="logo-1" src="~/site_tema/images/logo-light.png" alt="">
                                            <img class="logo-2" src="~/site_tema/images/logo.png" alt="">
                                        </a>
                                    </div>
                                    <!-- logo close -->
                                </div>
                            </div>
                            <div class="de-flex-col header-col-mid">
                                <ul id="mainmenu">
                                    <li>
                                        <a class="menu-item" asp-action="Index" asp-controller="Home">Ana Sayfa</a>
                                        
                                    </li>
                                    <li>
                                        <a class="menu-item" asp-action="MyCars" asp-controller="Home">Arabalarımız</a>
                                       
                                    </li>
                                    <li><a class="menu-item" asp-action="About" asp-controller="Home">Hakkımızda</a></li>
                                    <li><a class="menu-item" asp-action="Contact" asp-controller="Home">İletişim</a></li>
                                   
                                </ul>
                            </div>
                            <partial name="_rigthsidenav">

                            </partial>

                         
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <!-- header close -->

        <!-- content begin -->
        <div class="no-bottom no-top" id="content">
            <div id="top"></div>
           
                @RenderBody()
          

         
          
          

          

            

        </div>
        <!-- content close -->
        <a href="#" id="back-to-top"></a>
        <!-- footer begin -->
        <footer class="text-light">
            <div class="container">
                <div class="row g-custom-x">
                    <div class="col-lg-4">
                        <div class="widget">
                            <h5>Hakkımızda</h5>
                            <p>
                                Kalitenin uygun fiyatla buluştuğu yer. Aşırı maliyetlerin yükü olmadan sorunsuz ve keyifli bir yolculuğun önemini anlıyoruz. Bu nedenle, size minimum maliyetle birinci sınıf araçlar sunmak için tekliflerimizi titizlikle hazırladık.

                            </p>
                        </div>
                    </div>

                    <div class="col-lg-4">
                        <div class="widget">
                            <h5>İletişim Bilgilerimiz</h5>
                            <address class="s1">
                                <span><i class="id-color fa fa-map-marker fa-lg"></i>Antalya / Türkiye </span>
                                <span><i class="id-color fa fa-phone fa-lg"></i>+555 555 5555</span>
                                <span><i class="id-color fa fa-envelope-o fa-lg"></i><a href="mailto:contact@rentacar.com">contact@rentacar.com</a></span>
                            </address>
                        </div>
                    </div>

                   

                    <div class="col-lg-4">
                        <div class="widget">
                            <h5>Social Network</h5>
                            <div class="social-icons">
                                <a href="#"><i class="fa fa-facebook fa-lg"></i></a>
                                <a href="#"><i class="fa fa-twitter fa-lg"></i></a>
                                <a href="#"><i class="fa fa-linkedin fa-lg"></i></a>
                                <a href="#"><i class="fa fa-pinterest fa-lg"></i></a>
                                <a href="#"><i class="fa fa-rss fa-lg"></i></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="subfooter">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="de-flex">
                                <div class="de-flex-col">
                                    <a href="index.html">
                                        Copyright 2023 - Rentaly by Designesia
                                    </a>
                                </div>
                                <ul class="menu-simple">
                                    <li><a href="#">Terms &amp; Conditions</a></li>
                                    <li><a href="#">Privacy Policy</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
        <!-- footer close -->
    </div>

    <!-- Javascript Files
    ================================================== -->
    <script src="~/site_tema/js/plugins.js"></script>
    <script src="~/site_tema/js/designesia.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    @await Component.InvokeAsync("Notyf")
    @await RenderSectionAsync("Script", required: false)
</body>

</html>